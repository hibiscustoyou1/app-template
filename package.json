{
  "name": "app-template",
  "version": "1.0.0",
  "scripts": {
    "install:all": "pnpm i -r",
    "build:client": "cd client && pnpm build",
    "build:server": "cd server && pnpm build",
    "clean": "rm -rf dist",
    "prebuild": "pnpm clean",
    "build": "pnpm prebuild && pnpm build:client && pnpm build:server && pnpm move:dist",
    "move:dist": "mkdir -p dist && mv client/dist dist/client && mv server/dist dist/server",
    "start": "node dist/server/index.js"
  },
  "packageManager": "pnpm@10.21.0",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma"
    ]
  }
}
